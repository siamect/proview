	  Attr Flags |= PWR_MASK_STATE
        EndBody
      EndObject
      Object DataL_Dlid $Intern 32
        Body SysBody
          Attr PgmName = "DataL_Dlid"
          Attr TypeRef = "pwrs:Type-$RefId"
	  Attr Flags |= PWR_MASK_INVISIBLE
	  Attr Flags |= PWR_MASK_STATE
        EndBody
      EndObject
