!  pwrb_c_condamenoarr.wb_load -- Defines the class ConDaMeNoArr.
!
SObject pwrb:Class
  Object ConDaMeNoArr $ClassDef 43 
    Body SysBody
      Attr Flags |= pwr_mClassDef_DevOnly
    EndBody
    Object DevBody $ObjBodyDef 2 
      Object PlcConnection $Buffer 1 
        Body SysBody
	  Attr PgmName = "PlcCon"
          Attr Class = pwr_eClass_PlcConnection
        EndBody
      EndObject
    EndObject
    Object GraphPlcCon $GraphPlcConnection
      Body SysBody
        Attr con_type = 9
        Attr arrows = 0
        Attr linewidth = 0.005
        Attr dashes = 0.01
        Attr fillpattern = 0
        Attr curvature = 0
        Attr corners = .01
      EndBody
    EndObject
  EndObject
EndSObject
