!  pwrb_c_a_xttmethodsmask.wb_load -- Defines the class XttMethodsMask.
!
SObject pwrb:Class
!/**
!  @Version 1.0
!  @Summary Method masks for Xtt
!  Method masks for Xtt.
!*/
  Object XttMethodsMask $ClassDef 658
    Body SysBody
      Attr Editor = pwr_eEditor_AttrEd
      Attr Method = pwr_eMethod_Standard
      Attr PopEditor = 1
      Attr Flags |= pwr_mClassDef_Internal
    EndBody
    Object RtBody $ObjBodyDef 1 
      Body SysBody
        Attr StructName = "XttMethodsMask"
      EndBody
      !/**
      !  Method flags.
      !  IsConfigured indicates that the masks are configured and can be set
      !  in the development environment. If it's not set, the methods will be 
      !  configured in runtime the first time they are requested.
      !*/
      Object Flags $Attribute 1 
        Body SysBody
          Attr TypeRef = "pwrb:Type-XttMethodsFlagsMask"
        EndBody
      EndObject
      !/**
      !  Operator methods mask.
      !*/
      Object OpMethods $Attribute 2 
        Body SysBody
          Attr TypeRef = "pwrb:Type-XttOpMethodsMask"
        EndBody
      EndObject
      !/**
      !  Maintenace methods mask.
      !*/
      Object MntMethods $Attribute 3 
        Body SysBody
          Attr TypeRef = "pwrb:Type-XttMntMethodsMask"
        EndBody
      EndObject
    EndObject
  EndObject
EndSObject
