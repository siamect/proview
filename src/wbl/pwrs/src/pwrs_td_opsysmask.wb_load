!  pwrs_td_opsysmask.wb_load -- Defines the type OpSysMask
!
SObject pwrs:Type
!/**
!  @Version 1.0
!  @Code pwr.h
!  @Group DerivedTypes,Types
!  Operating system bitmask. Derived from Mask.
!
! @b See also
!  @classlink Mask pwrs_mask.html
!*/
  Object $OpSysMask $TypeDef 14
    Body SysBody
      Attr TypeRef = "pwrs:Type-$Mask"
      Attr PgmName = "OpSysMask"
    EndBody
    !/**
    !  Operating system is defined in CustomBuild
    !*/
    Object CustomBuild $Bit
     Body SysBody
       Attr PgmName = "CustomBuild"
       Attr Text = "CustomBuild"
       Attr Value = 1
     EndBody
    EndObject
    !/**
    !  Operating system is Linux on PowerPC
    !*/
    Object PPC_LINUX $Bit
     Body SysBody
       Attr PgmName = "PPC_LINUX"
       Attr Text = "Linux on PowerPC"
       Attr Value = 32
     EndBody
    EndObject
    !/**
    !  Operating system is Linux on x86
    !*/
    Object X86_LINUX $Bit
     Body SysBody
       Attr PgmName = "X86_LINUX"
       Attr Text = "Linux on x86"
       Attr Value = 64
     EndBody
    EndObject
    !/**
    !  Operating system is Linux on x86_64
    !*/
    Object X86_64_LINUX $Bit
     Body SysBody
       Attr PgmName = "X86_64_LINUX"
       Attr Text = "Linux on x86_64"
       Attr Value = 128
     EndBody
    EndObject
    !/**
    !  Operating system is Mac OS X on x86_64
    !*/
    Object X86_64_MACOS $Bit
     Body SysBody
       Attr PgmName = "X86_64_MACOS"
       Attr Text = "Mac OS X on x86_64"
       Attr Value = 256
     EndBody
    EndObject
    !/**
    !  Operating system is Linux on ARM
    !*/
    Object ARM_LINUX $Bit
     Body SysBody
       Attr PgmName = "ARM_LINUX"
       Attr Text = "Linux on ARM"
       Attr Value = 512
     EndBody
    EndObject
    !/**
    !  Operating system is FreeBSD on x86_64
    !*/
    Object X86_64_FREEBSD $Bit
     Body SysBody
       Attr PgmName = "X86_64_FREEBSD"
       Attr Text = "FreeBSD on x86_64"
       Attr Value = 1024
     EndBody
    EndObject
    !/**
    !  Operating system is OpenBSD on x86_64
    !*/
    Object X86_64_OPENBSD $Bit
     Body SysBody
       Attr PgmName = "X86_64_OPENBSD"
       Attr Text = "OpenBSD on x86_64"
       Attr Value = 2048
     EndBody
    EndObject
    !/**
    !  Operating system is Cygwin on x86
    !*/
    Object X86_CYGWIN $Bit
     Body SysBody
       Attr PgmName = "X86_CYGWIN"
       Attr Text = "Cygwin on x86"
       Attr Value = 4096
     EndBody
    EndObject
    !/**
    !  Operating system is Linux on ARM64
    !*/
    Object ARM64_LINUX $Bit
     Body SysBody
       Attr PgmName = "ARM64_LINUX"
       Attr Text = "Linux on ARM64"
       Attr Value = 8192
     EndBody
    EndObject
  EndObject
EndSObject
